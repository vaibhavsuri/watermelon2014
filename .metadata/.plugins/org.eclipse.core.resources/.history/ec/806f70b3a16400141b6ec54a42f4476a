package watermelon.group5;

public class Seed {
  public double x;
  public double y;
  public boolean tetraploid;
  public double score;
  public Ploidy ploidy;
  public Seed() { x = 0.0; y = 0.0; ploidy = Ploidy.NONE; }

  public Seed(double xx, double yy, boolean tetra) {
    x = xx;
    y = yy;
    ploidy = Ploidy.TETRA;
    tetraploid = true;
  }
}
